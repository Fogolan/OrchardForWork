@{
    var token = TempData["Token"];
    var isDeleteToken = TempData["IsDeleteToken"];
    Style.Require("TestApi.ShoppingCart");
    Script.Require("TestApi.ShoppingCart");
    Script.Require("TestApi.RefreshToken");
    var tokenShape = New.TokenShape(Token: token, IsDeleteToken: isDeleteToken);
}
<article class="shoppingcart">
    <div id="tableBlock"></div>
    <br/>
        @Display(tokenShape)
</article>