@{
    var token = Model.Token;
    var isDeleteToken = Model.IsDeleteToken;
}
@if (!string.IsNullOrEmpty(token)) {
    <script>
        var tokenString = "@token".replace(/&quot;/g, '"');
        sessionStorage.setItem("tokenInfo", tokenString);
        console.log("getItem", sessionStorage.getItem("tokenInfo"));
    </script>
}
@if (isDeleteToken == true)
{
    <script>
            var tokenKey = "tokenInfo";
            if (sessionStorage.getItem(tokenKey) != null) {
                    sessionStorage.removeItem(tokenKey);
                }
        </script>
}
